    <div class="login-panel panel panel-default">
        <div class="panel-heading ">
            
            <h3 class="panel-title">Auto Safe</h3>
            <blockquote>
                Insira as informações solici.
            </blockquote>
            <div class="panel-body">


                <div class="row">
                    <form class="col s12" action="@Url.Action("EnviaOficina","Chamado")" method="post" >
                        <div class="row">
                            <div class="input-field col s6">
                                <p>
                                    <input type="checkbox" class="filled-in" id="inputBo" name="inputBo" />
                                    <label for="inputBo">Boletim  Ocorrência</label>
                                </p>

                            </div>
                            <div class="input-field col s6">
                                <p>
                                    <input type="checkbox" class="filled-in" id="inputCompRes" name="inputCompRes" />
                                    <label for="inputCompRes">Comprovante de Residência</label>
                                </p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s12">
                                <p>
                                    <input type="checkbox" class="filled-in" id="inputCnh" name="inputCnh" />
                                    <label for="inputCnh">Carteira de Nacional de Habilitação</label>
                                </p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s12">
                                <p>
                                    <input type="checkbox" class="filled-in" id="inputLiberaEletrica" name="inputLiberaEletrica" />
                                    <label for="inputLiberaEletrica">Liberar Elétrica</label>
                                </p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s12">
                                <p>
                                    <input type="checkbox" class="filled-in" id="inputLiberaMecanica" name="inputLiberaMecanica" />
                                    <label for="inputLiberaMecanica">Liberar Mecânica</label>
                                </p>
                            </div>                        </div>
                        <div class="row">
                            <div class="input-field col s12">
                                <p>
                                    <input type="checkbox" class="filled-in" id="inputLiberarFunilaria" name="inputLiberarFunilaria" />
                                    <label for="inputLiberarFunilaria">Liberar Funilaria</label>
                                </p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s6">
                                <p>
                                    <textarea id="taEletrica" class="materialize-textarea" name="taEletrica"></textarea>
                                    <label for="taEletrica">Insira as informações de Elétrica</label>
                                </p>
                            </div>
                            <div class="input-field col s6">
                                <p>
                                    <textarea id="taMecania" class="materialize-textarea" name="taMecania"></textarea>
                                    <label for="taMecania">Insira as informações de Mecânica</label>
                                </p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s6">
                                <p>
                                    <textarea id="taFunilaria" class="materialize-textarea" name="taFunilaria"></textarea>
                                    <label for="taFunilaria">Insira as informações de Funilaria</label>
                                </p>
                            </div>

                            <div class=" row">

                                <div class="input-field col s6">

                                    <button class="btn waves-effect waves-light" type="submit" name="action">
                                        Enviar para Oficina
                                        <i class="material-icons right">send</i>
                                    </button>
                                </div>

                            </div>
                    </form>
                    <div class="input-field col s6">
                        <button class="btn waves-effect waves-light" type="submit" onclick="@Url.Action("Cancelar","Chamado")" name="cancel">
                            Cancelar chamado
                            <i class="material-icons">do_not_disturb_on</i>

                        </button>
                    </div>


                    </div>



                </div>



            </div>
    </div>
    </div>
    </div>


